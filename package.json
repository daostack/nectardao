{
  "name": "nectardao",
  "version": "1.0.0",
  "description": "nectar dao deployment repo",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "migrate_production": "daostack-migrate dao --prev-migration node_modules/@daostack/migration/migration.json --gasPrice 30 --params ./parameters/production.json",
    "prepare": "node ./prepare.js ./parameters/deployment.json ./parameters/mainnet_params_11_11.json",
    "migrate": "npm run prepare && npm explore @daostack/migration -- npm i @daostack/arc@0.0.1-rc.30 && npm explore @daostack/migration -- npm run migrate.dao -- --gasPrice 30 --params ../../../params.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/daostack/nectardao.git"
  },
  "keywords": [
