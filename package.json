{
  "name": "nectardao",
  "version": "1.0.0",
  "description": "nectar dao deployment repo",
  "main": "index.js",
  "scripts": {
    "migrate_prod": "npm run prepare_prod && daostack-migrate dao --prev-migration node_modules/@daostack/migration/migration.json --gasPrice 20 --params ./params.json --custom-abis-location ./custom-abis --arc-version 0.0.1-rc.33",
    "prepare": "node ./prepare.js ./parameters/deployment.json ./parameters/mainnet_params_25_11.json",
    "migrate": "npm run prepare && daostack-migrate dao --prev-migration node_modules/@daostack/migration/migration.json --gasPrice 20 --params ./params.json --custom-abis-location ./custom-abis --arc-version 0.0.1-rc.33",
    "redeem": "node -e 'require(\"./scripts/nectarRedeem.js\").startRedeem()'",
    "test": "jest",
    "ganache": "npm explore @daostack/migration -- npm install && npm explore @daostack/migration -- npm run ganache",
    "migrate-ganache": "daostack-migrate --prev-migration node_modules/@daostack/migration/migration.json --gasPrice 20 --params test/test-parameters.json --arc-version 0.0.1-rc.33 --force --restart"
    "prepare_prod": "node ./prepare.js ./parameters/deployment_prod.json ./parameters/production_mainnet_params.json",
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/daostack/nectardao.git"
  },
  "keywords": [
    "nectar",
    "dao",
    "daostack"
  ],
  "author": "daostack.io",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/daostack/nectardao/issues"
  },
  "homepage": "https://github.com/daostack/nectardao#readme",
  "dependencies": {
    "@daostack/migration": "0.0.1-rc.33-v1",
    "dotenv": "^8.2.0",
    "unix-timestamp": "^0.2.0",
    "web3": "1.0.0-beta.55"
  },
  "devDependencies": {
    "jest": "^24.9.0"
  }
}
