{
  "name": "nectardao",
  "version": "1.0.0",
  "description": "nectar dao deployment repo",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "migrate_production": "daostack-migrate dao --prev-migration node_modules/@daostack/migration/migration.json --gasPrice 30 --params ./parameters/production.json",
    "prepare": "node ./prepare.js ./parameters/deployment.json ./parameters/mainnet_params_25_11.json",
    "migrate": "npm run prepare && daostack-migrate dao --prev-migration node_modules/@daostack/migration/migration.json --gasPrice 40 --params ./params.json --custom-abis-location ./custom-abis --arc-version 0.0.1-rc.33",
    "redeem": "node -e 'require(\"./scripts/redeemRep.js\").startRedeem()'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/daostack/nectardao.git"
  },
  "keywords": [
    "nectar",
    "dao",
    "daostack"
  ],
  "author": "daostack.io",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/daostack/nectardao/issues"
  },
  "homepage": "https://github.com/daostack/nectardao#readme",
  "dependencies": {
    "@daostack/migration": "0.0.1-rc.33-v1",
    "dotenv": "^8.2.0",
    "unix-timestamp": "^0.2.0",
    "web3": "1.0.0-beta.55"
  }
}
